bin_age <- function(df, col){
col <- enquo(col)
df %>%
dplyr::mutate(age_bin :=
dplyr::case_when(
!!col <  10 ~ 'Under 10',
!!col >= 10 & !!col < 20  ~  '10-19',
!!col >= 20 & !!col < 30	~  '20-29',
!!col >= 30 & !!col < 40	~  '30-39',
!!col >= 40 & !!col < 50	~  '40-49',
!!col >= 50 & !!col < 60	~  '50-59',
!!col >= 60 & !!col < 70	~  '60-69',
!!col >= 70 & !!col < 80	~  '70-79',
!!col >  80 ~ 'Over 80') %>%
return()
)
}
